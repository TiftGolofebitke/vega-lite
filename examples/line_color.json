{
  "description": "Colored Line chart of Horsepower for Cars from different Origin over the years.",
  "data": {
    "url": "data/cars.json"
  },
  "mark": "line",
  "encoding": {
    "x": {"timeUnit": "year", "field": "Year", "type": "temporal"},
    "y": {"aggregate": "mean", "field": "Horsepower", "type": "quantitative"},
    "color": {"field": "Origin", "type": "nominal"}
  },
  "config": {"mark": {"interpolate": "monotone"}}
}
